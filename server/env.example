# Copy this file to .env and fill in your values
# cp env.example .env

# TwelveLabs
TWELVELABS_API_KEY=your-api-key

# Default autism markers to detect (comma-separated)
DEFAULT_MARKERS=hand_flapping,rocking,spinning,toe_walking,limited_eye_contact,lack_of_social_engagement,echolalia,limited_verbal_response,covering_ears,unusual_sensory_response,object_lining,repetitive_play

# Video processing (optional)
CHUNK_DURATION_SECONDS=900
CHUNK_OVERLAP_SECONDS=10

# CORS origins (optional)
CORS_ORIGINS=http://localhost:5173,http://localhost:3000
